---
import Layout from "../layouts/Layout.astro";
import ColabCard from "../components/Colab_card.astro";
import { getCollection, render } from "astro:content";
const contributors = await getCollection("contributors");
//const { Content } = await render(contributors);
export const prerender = true;
---

<Layout title="kukulc4n â€¢ All Projects">
    <p class="text-white text-center text-4xl p-4">
        {
            contributors.map((contributor) => (
                <ColabCard
                    colab_name={contributor.data.colab_name}
                    tec={contributor.data.tec}
                    email={contributor.data.email}
                    user_img={contributor.data.user_img}
                />
            ))
        }
    </p>
</Layout>
