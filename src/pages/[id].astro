---
import Layout from "../layouts/Layout.astro";
import { getCollection, render } from "astro:content";
export async function getStaticPaths() {
    const contributors = await getCollection("contributors");
    return contributors.map((contributor) => ({
        params: {
            id: contributor.slug,
        },
        props: {
            contributor,
        },
    }));
}
const { contributor } = Astro.props;
const { data } = contributor;
const { colab_name, tec, email } = data;
const { Content } = await render(contributor);
export const prerender = true;
---

<Layout title="Kukulc4n â€¢ Contributors">
    <h1>{colab_name}</h1>
    <p>{tec}</p>
    <p>{email}</p>
    <Content />
</Layout>
